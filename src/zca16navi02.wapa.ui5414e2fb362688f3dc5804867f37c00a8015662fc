sap.ui.define([                                                                                                                                                                                                                                                
    "sap/ui/core/mvc/Controller",                                                                                                                                                                                                                              
    "sap/ui/model/Filter"                                                                                                                                                                                                                                      
],                                                                                                                                                                                                                                                             
    /**                                                                                                                                                                                                                                                        
     * @param {typeof sap.ui.core.mvc.Controller} Controller                                                                                                                                                                                                   
     */                                                                                                                                                                                                                                                        
    function (Controller, Filter) {                                                                                                                                                                                                                            
        "use strict";                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
        return Controller.extend("sync.navi02.controller.View1", {                                                                                                                                                                                             
            onInit: function () {                                                                                                                                                                                                                              
                let oComponentData = this.getOwnerComponent().getComponentData();                                                                                                                                                                              
                if (oComponentData && oComponentData.startupParameters && oComponentData.startupParameters.pCarrid) {                                                                                                                                          
                    this._carrid = oComponentData.startupParameters.pCarrid[0];                                                                                                                                                                                
                }                                                                                                                                                                                                                                              
            },                                                                                                                                                                                                                                                 
            onAfterRendering: function () {                                                                                                                                                                                                                    
                let oFilter = new Filter("Carrid", "EQ", this._carrid);                                                                                                                                                                                        
                let oTable = this.getView().byId("idSpfli");                                                                                                                                                                                                   
                let oBinding = oTable.getBinding("items");                                                                                                                                                                                                     
                oBinding.filter(oFilter);                                                                                                                                                                                                                      
            }                                                                                                                                                                                                                                                  
        });                                                                                                                                                                                                                                                    
    });                                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               